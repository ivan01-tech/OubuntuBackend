{
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc",
    "lint": "eslint \"src\"",
    "inspect": "nodemon --inspect=9229 dist/app.js",
    "start": "concurrently -K -n \"Typescript,Node\" -p \"[{name}]\" -c \"blue,cyan\" \"tsc --watch\" \"nodemon dist/app.js\" 2> error.log",
    "dev": "concurrently -K -n \"Typescript,Node\" -p \"[{name}]\" -c \"blue,cyan\" \"tsc --watch\" \"nodemon dist/app.js\"",
    "start:app": "dotenv -c '.env' -- node dist/app"
  },
  "dependencies": {
    "@adminjs/express": "^6.1.0",
    "@adminjs/mongoose": "^4.0.0",
    "@adminjs/passwords": "^4.0.0",
    "adminjs": "^7.4.1",
    "bcrypt": "^5.1.1",
    "concurrently": "^8.2.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.2",
    "express": "^4.18.2",
    "express-formidable": "^1.2.0",
    "express-session": "^1.17.3",
    "libphonenumber-js": "^1.10.55",
    "mongoose": "^7.6.6",
    "nodemon": "^3.0.3",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/express-rate-limit": "^6.0.0",
    "@types/express-session": "^1.17.10",
    "@types/node": "^18.15.11",
    "@types/passport": "^1.0.16",
    "@types/passport-google-oauth20": "^2.0.14",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.38.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.1.0",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4"
  },
  "name": "oubuntu2"
}
